{
    "version": 2,
    "name": "fastify-prisma",
    "builds": [
      { "src": "src/app.ts", "use": "@vercel/node" }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/src/app.ts"
      }
    ],
    "functions": {
    "node": {
      "runtime": "nodejs18.x"
    }
  }
}